{"components/common/currency-selector":"{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\" href=\"#\" data-dropdown=\"currencySelection\" aria-controls=\"currencySelection\" aria-expanded=\"false\">{{lang 'common.currency' code=currency_selector.active_currency_code}} <i class=\"icon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i></a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\">\n                    {{#if is_active}}\n                    <strong>{{name}}</strong>\n                    {{else}}\n                    {{name}}\n                    {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n","components/common/custom-navigation":"<div id=\"covid101\" class=\"modalcovid101\">\r\n<!--  <span onclick=\"document.getElementById('covid101').style.display='none'\" class=\"closemodalcovid101\" title=\"Close Modal\">×</span>-->\r\n  <form class=\"modalcovid101-content\">\r\n    <div style=\"text-align:center;padding-bottom: 2%;\">\r\n      <h1 style=\"\r\n        font-weight: 700;\r\n    background-color: #d22229;\r\n    font-size: 30px;\r\n    margin: 0;\r\n    color: #fff;\r\n    padding-top: 1%;\r\n    padding-bottom: 1%;\r\n\">KINGS COVID 19 ON STOCKS NOTICE</h1>\r\n      <p style=\"\r\n    font-size: 18px;\r\n    color: #444444;\r\n    padding: 2% 3% 0 3%;\r\n\">Because of the impact of Covid-19 on the global supply chain, some items may not appear in the store or listed as out of stock. If you are not seeing an item of interest, please contact or visit a local King’s store near you and we’ll be happy to help as much as we can!</p>\r\n        <a class=\"modalcovid-button\" href=\"/locations/\">FIND A STORE</a>\r\n    \r\n\r\n    </div>\r\n  </form>\r\n</div>\r\n\r\n<script>\r\n// Get the modal\r\nvar modal = document.getElementById('covid101');\r\n\r\n// When the user clicks anywhere outside of the modal, close it\r\nwindow.onclick = function(event) {\r\n  if (event.target == modal) {\r\n    modal.style.display = \"none\";\r\n  }\r\n}\r\n</script>\r\n<nav class=\"navUser\">\r\n    {{> components/common/currency-selector}}\r\n<div class=\"header-text\">\r\n    <span>Missing some items? <button class=\"covid101button\" onclick=\"document.getElementById('covid101').style.display='block'\">Learn more about the effects of COVID-19 on our supplies here.</button></span>\r\n</div>\r\n    \r\n   \r\n\r\n\r\n    \r\n    <ul class=\"custom-nav-banner navUser-section navUser-section--alt\">\r\n        \r\n            <li class=\"navUser-item\">\r\n            <a class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/location/\">Locations</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/disclaimer/\">Disclaimer</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/rebates/\">Rebates</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/current-ads/\">Current Ads</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/blog/\">&nbsp; Blog &nbsp;</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/contact-us/\">Contact Us</a>\r\n            </li>\r\n\r\n    </ul>\r\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n</nav>\r\n","components/common/header":"<link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\r\n<header class=\"header\" role=\"banner\">\r\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\r\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\r\n    </a>\r\n    {{> components/common/custom-navigation}}\r\n<div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\r\n    <div class=\"header-logo-section\">\r\n        <div>\r\n            {{> components/common/store-logo}}\r\n           \r\n        </div> \r\n        <div class=\"search-logo_header\">\r\n             {{> components/common/search-header}}\r\n        </div>\r\n        <div>\r\n            {{> components/common/navigation}}\r\n        </div>\r\n    </div>\r\n</div>\r\n    <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n        {{> components/common/logo-navigation}}\r\n    </div>\r\n    \r\n   \r\n    \r\n<div id=\"stick-navbar\">\r\n\r\n    <div class=\"navPages-container\" id=\"menu\" data-menu>\r\n        {{> components/common/logo-navigation}}\r\n    </div>\r\n</div>\r\n    \r\n</header>\r\n<!--\r\n<div class=\"Covid-banner\">\r\n  <img src=\"https://cdn11.bigcommerce.com/s-bek4r4rsz5/product_images/uploaded_images/covid-png.png?t=1596000700&_ga=2.87819933.466510793.1595982495-2022119561.1571639426\">  <p>Due to COVID 19 product supply has dropped worldwide. If you are not seeing an item of interest please <a href=\"/locations\">contact your local King's</a> and we will be glad to help to the best of our abilities!</p>\r\n</div>\r\n-->\r\n\r\n<!--\r\n<div class=\"Promo-Banner\">\r\n<img src=\"