 </li>\r\n        <li class=\"navPages-item\">\r\n                <a href=\"/deal-match/\" class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" style=\"white-space:nowrap;\">\r\n                    <!-- <img class=\"nav-image\" src=\"{{getImage (cdn 'img/homepage/deal-match.png')}}\"  /> -->\r\n                    <span style=\"white-space: nowrap;\">Deal Match</span>\r\n                   \r\n                </a>\r\n        </li>\r\n        \r\n    \r\n        \r\n        \r\n    </ul>\r\n\r\n</nav>\r\n<script type=\"text/javascript\" src=\"https://code.jquery.com/git/jquery-1.x-git.min.js\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jquery-migrate/1.2.1/jquery-migrate.min.js\" integrity=\"sha512-fDGBclS3HUysEBIKooKWFDEWWORoA20n60OwY7OSYgxGEew9s7NgDaPkj7gqQcVXnASPvZAiFW8DiytstdlGtQ==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js\" integrity=\"sha512-XtmMtDEcNz2j7ekrtHvOVR4iwwaD6o/FUJe6+Zq+HgcCsk3kj4uSQQR8weQ2QVj1o0Pk6PwYLohm206ZzNfubg==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>\r\n<script>\r\n\r\n    var slick_option = {\r\n        \"dots\": false,\r\n        \"mobileFirst\": true,\r\n        \"slidesToShow\": 5,\r\n        \"slidesToScroll\": 5,\r\n        \"autoplay\": false,\r\n\t\t\"infinite\": false\r\n    };\r\n\t\t\t\r\n\t\t\tjQuery('.gg').each(function(){\r\n\t\t\t\tjQuery(this).not('.slick-initialized').slick(slick_option);\r\n\t\t\t});\r\n\t\t\t\r\n\t\t\t\r\n\r\n\r\njQuery('.navPages-action.has-subMenu').on('hover click mousemove' , function(){\r\n\tvar _slickContainer = jQuery(this).parent().find('.gg');\r\n\t\r\n      _slickContainer[0].slick.refresh();\t \r\n    \r\n\r\n});\r\n\r\n\r\n\r\n$(window).on('resize orientationchange', function() {\r\n  $('.gg').slick('resize');\r\n});\r\n\r\n\r\n//delete element if  innerhtml of navpage is empty\r\n    var elem = document.getElementsByClassName('navPages-item');\r\n    for(let i = 0; i<=elem.length;i++){\r\n        var htmlstring = elem[i].innerHTML;\r\n        htmlstring = (htmlstring.trim) ? htmlstring.trim() : htmlstring.replace(/^\\s+/,'');\r\n        if(htmlstring == ''){\r\n            elem[i].remove();\r\n        }\r\n    }\r\n</script>","components/common/navigation":"\r\n<nav class=\"navUser\">\r\n    {{> components/common/currency-selector}}\r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n            <li class=\"navUser-item\">\r\n                <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/location.png')}}\"> -->\r\n                <a href=\"/location/\"><i class=\"fa fa-map-marker navigation_icons\" aria-hidden=\"true\"></i></a>  \r\n                <a class=\"navUser-action\" href=\"/location/\">Find a Store</a>\r\n            </li>\r\n\r\n        <li class=\"navUser-item navUser-item--account\">\r\n            <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/account.png')}}\"> -->\r\n                <a href=\"{{urls.account.index}}\"><i class=\"fa fa-user-circle-o navigation_icons\" aria-hidden=\"true\"></i></a>\r\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a>\r\n        </li>\r\n        \r\n        <li class=\"navUser-item navUser-item--cart\">\r\n             <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/cart.png')}}\"> -->\r\n             <a href=\"{{urls.cart}}\"><i class=\"fa fa-shopping-cart navigation_icons\" aria-hidden=\"true\"></i></a>\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                data-dropdown=\"cart-preview-dropdown\"\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span> <span class=\"countPill cart-quantity\"></span>\r\n        