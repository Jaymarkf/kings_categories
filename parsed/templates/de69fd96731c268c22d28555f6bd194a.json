}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n","components/common/navigation-list-new":"{{#if children}}\r\n        <a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" style=\"white-space:nowrap;\">\r\n            <!-- <img class=\"nav-image\" src=\"{{cdn (concat 'img/homepage/' (concat name '.png'))}}\"  /> -->\r\n            <span style=\"white-space: nowrap;\">{{name}}</span>\r\n            <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\r\n        </a>\r\n\r\n    <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <div class=\"navChild_name\">\r\n            <h2>{{name}}</h2>\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\" style=\"margin-left:5% !important;\">SHOP ALL \r\n                    <img src=\"https://cdn11.bigcommerce.com/s-bek4r4rsz5/product_images/uploaded_images/chevron-right-solid-red-.png\">\r\n                </a>\r\n        </div> \r\n        <ul class=\"gg\">\r\n            {{#each children}}\r\n                <li>\r\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n                            <img class=\"\" src=\"{{cdn (concat 'img/nav/' (concat  (lowercase (dashcase name)) '.png'))}}\"  />\r\n                            <span style=\"margin-left:10%;\">{{name}}</span>\r\n                        </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul> \r\n    </div>\r\n{{else}}\r\n    {{#if name '==' 'Delivery Express'}}\r\n        <a href=\"/delivery-express/\" class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" style=\"white-space:nowrap;margin-right:2%;margin-left:1%;\">\r\n            <!-- <img class=\"nav-image\" src=\"{{cdn 'assets/img/homepage/delivery-express.png'}}\"  />  -->\r\n            <span style=\"white-space: nowrap;\">{{name}}</span>\r\n        </a>\r\n    {{/if}}\r\n{{/if}}\r\n\r\n\r\n","components/common/quick-search":"<div class=\"container\">\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n","components/common/search-header":"<form class=\"form\"  action=\"{{urls.search}}\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"search_query_adv\">{{lang 'search.results.form_label'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\" style=\"background-color:white;\">\r\n                <input class=\"form-input\" placeholder=\"What can we help you find?\" id=\"search_query_adv\" name=\"search_query_adv\" value=\"{{forms.search.query}}\">\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">\r\n            </div>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n"}
