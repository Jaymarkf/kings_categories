{"components/common/logo-navigation":"\r\n\r\n<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n    \r\n\r\n    <ul id=\"myUL\" class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        {{#each categories}}\r\n                \r\n          \r\n        \r\n                \r\n            {{#if ../theme_settings.navigation_design '===' 'simple'}}\r\n             {{#all (if name '!==' 'Rebates') (if name '!==' 'Appliances') }}\r\n                <li class=\"navPages-item\">\r\n                {{> components/common/navigation-list-new}}\r\n                 </li>\r\n                  {{/all}}\r\n            {{/if}}\r\n        \r\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n             {{#all (if name '!==' 'Rebates') (if name '!==' 'Appliances') }}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/navigation-list-alternate}}\r\n                   </li>\r\n                  {{/all}}\r\n            {{/if}}\r\n        \r\n  \r\n        \r\n        {{/each}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"/brands/\" style=\"white-space:nowrap;\">\r\n                    <!-- <img class=\"nav-image\" src=\"{{getImage (cdn 'img/homepage/Brands.png')}}\"  /> -->\r\n                   <span style=\"white-space:nowrap\">Brands</span>\r\n                \r\n                </a>\r\n            </li>\r\n        <li class=\"navPages-item\">\r\n                <a href=\"/deal-match/\" class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" style=\"white-space:nowrap;\">\r\n                    <!-- <img class=\"nav-image\" src=\"{{getImage (cdn 'img/homepage/deal-match.png')}}\"  /> -->\r\n                    <span style=\"white-space: nowrap;\">Deal Match</span>\r\n                   \r\n                </a>\r\n        </li>\r\n        \r\n    \r\n        \r\n        \r\n    </ul>\r\n\r\n</nav>\r\n<script type=\"text/javascript\" src=\"https://code.jquery.com/git/jquery-1.x-git.min.js\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jquery-migrate/1.2.1/jquery-migrate.min.js\" integrity=\"sha512-fDGBclS3HUysEBIKooKWFDEWWORoA20n60OwY7OSYgxGEew9s7NgDaPkj7gqQcVXnASPvZAiFW8DiytstdlGtQ==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js\" integrity=\"sha512-XtmMtDEcNz2j7ekrtHvOVR4iwwaD6o/FUJe6+Zq+HgcCsk3kj4uSQQR8weQ2QVj1o0Pk6PwYLohm206ZzNfubg==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>\r\n<script>\r\n\r\n    var slick_option = {\r\n        \"dots\": false,\r\n        \"mobileFirst\": true,\r\n        \"slidesToShow\": 5,\r\n        \"slidesToScroll\": 5,\r\n        \"autoplay\": false,\r\n\t\t\"infinite\": false\r\n    };\r\n\t\t\t\r\n\t\t\tjQuery('.gg').each(function(){\r\n\t\t\t\tjQuery(this).not('.slick-initialized').slick(slick_option);\r\n\t\t\t});\r\n\t\t\t\r\n\t\t\t\r\n\r\n\r\njQuery('.navPages-action.has-subMenu').on('hover click mousemove' , function(){\r\n\tvar _slickContainer = jQuery(this).parent().find('.gg');\r\n\t\r\n      _slickContainer[0].slick.refresh();\t \r\n    \r\n\r\n});\r\n\r\n\r\n\r\n$(window).on('resize orientationchange', function() {\r\n  $('.gg').slick('resize');\r\n});\r\n\r\n\r\n//delete element if  innerhtml of navpage is empty\r\n    var elem = document.getElementsByClassName('navPages-item');\r\n    for(let i = 0; i<=elem.length;i++){\r\n        var htmlstring = elem[i].innerHTML;\r\n        htmlstring = (htmlstring.trim) ? htmlstring.trim() : htmlstring.replace(/^\\s+/,'');\r\n        if(htmlstring == ''){\r\n            elem[i].remove();\r\n        }\r\n    }\r\n</script>","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list-alternate":"{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n","components/common/navigation-list-new":"{{#if children}}\r\n        <a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" style=\"white-space:nowrap;\">\r\n            <!-- <img class=\"nav-image\" src=\"{{cdn (concat 'img/homepage/' (concat name '.png'))}}\"  /> -->\r\n            <span style=\"white-space: nowrap;\">{{name}}</span>\r\n            <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\r\n        </a>\r\n\r\n    <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <div class=\"navChild_name\">\r\n            <h2>{{name}}</h2>\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\" style=\"margin-left:5% !important;\">SHOP ALL \r\n                    <img src=\"https://cdn11.bigcommerce.com/s-bek4r4rsz5/product_images/uploaded_images/chevron-right-solid-red-.png\">\r\n                </a>\r\n        </div> \r\n        <ul class=\"gg\">\r\n            {{#each children}}\r\n                <li>\r\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n                            <img class=\"\" src=\"{{cdn (concat 'img/nav/' (concat  (lowercase (dashcase name)) '.png'))}}\"  />\r\n                            <span style=\"margin-left:10%;\">{{name}}</span>\r\n                        </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul> \r\n    </div>\r\n{{else}}\r\n    {{#if name '==' 'Delivery Express'}}\r\n        <a href=\"/delivery-express/\" class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" style=\"white-space:nowrap;margin-right:2%;margin-left:1%;\">\r\n            <!-- <img class=\"nav-image\" src=\"{{cdn 'assets/img/homepage/delivery-express.png'}}\"  />  -->\r\n            <span style=\"white-space: nowrap;\">{{name}}</span>\r\n        </a>\r\n    {{/if}}\r\n{{/if}}\r\n\r\n\r\n","components/common/quick-search":"<div class=\"container\">\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n"}
