{"components/common/VisitUs":"<div class=\"locationRow row\">\r\n\r\n  <div class=\"container\">\r\n\r\n\r\n  <iframe src=\"https://player.vimeo.com/video/320327484\" width=\"640\" height=\"360\" allowfullscreen=\"allowfullscreen\"></iframe>\r\n  </div>\r\n\r\n</div>\r\n\r\n\r\n<div class=\"ourBrands\">\r\n\r\n  <h1 class=\"visitusTitle\">OUR BRANDS</h1>\r\n  <p class=\"visitDescription\">King’s Great Buys carry the world’s most popular brands for appliances, electronics, furniture, and mattresses—names that you know and trust.</p>\r\n\r\n<div class=\"brandImage\">\r\n\r\n\r\n\r\n\r\n  <div data-slick='{\"slidesToShow\": 6, \"slidesToScroll\": 3}' class=\"imageBrand\">\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Bosch.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Electrolux.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/LG.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/NECDisplay.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Samsung.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Sharp.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Sony.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Philips.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Presto.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/T-fal.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Thermador.png'}}\" alt=\"\"></div>\r\n    <div><img class=\"brandsection\" src=\"{{cdn 'img/Toshiba.png'}}\" alt=\"\"></div>\r\n</div>\r\n\r\n</div>\r\n\r\n\r\n\r\n\r\n</div>\r\n<div class=\"subscriptionRow\">\r\n\r\n  {{> components/common/subscription-form2}}\r\n\r\n</div>\r\n","components/common/subscription-form2":"<h2 class=\"SUbscriptionHeading\">{{lang 'newsletter.subscribe'}}</h2>\r\n\r\n\r\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\r\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\r\n        <input type=\"hidden\" name=\"check\" value=\"1\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\">\r\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"{{lang 'newsletter.email_placeholder'}}\">\r\n                <input class=\"subsbutton button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\r\n            </div>\r\n            {{#if settings.show_newsletter_summary }}\r\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\r\n            {{/if}}\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n"}
