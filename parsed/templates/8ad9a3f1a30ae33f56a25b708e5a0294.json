{"components/common/footer":"{{#if banners.bottom}}\r\n    {{#if settings.data_tag_enabled}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            {{#each (limit banners.bottom_metadata 1)}}\r\n                <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-name=\"{{this.banner-name}}\">\r\n                    <div data-event-type=\"promotion-click\">\r\n                        {{{this.content}}}\r\n                    </div>\r\n                </div>\r\n            {{/each}}\r\n        </div>\r\n    {{else}}\r\n        <div class=\"banners\" data-banner-location=\"bottom\">\r\n            <div class=\"banner\">\r\n                {{{limit banners.bottom 1}}}\r\n            </div>\r\n        </div>\r\n    {{/if}}\r\n\r\n{{/if}}\r\n\r\n<!-- for compare products  after checking the add to compare -->\r\n{{> components/products/sticky-product-compare}}\r\n<div class=\"newsletter-home-section\">\r\n \r\n{{> components/common/subscription-form}}\r\n\r\n</div>\r\n<footer class=\"footer\" role=\"contentinfo\">\r\n    <div class=\"container\">\r\n        {{#if theme_settings.social_icon_placement_bottom '!==' 'bottom_none'}}\r\n            <article class=\"footer-info-col\r\n                footer-info-col--social\r\n                footer-info-col--{{#if theme_settings.social_icon_placement_bottom '===' 'bottom_left'}}left{{else}}right{{/if}}\"\r\n                data-section-type=\"footer-webPages\">\r\n                    <h5 class=\"footer-info-heading\">{{lang 'social.connect'}}</h5>\r\n                    {{> components/common/social-links}}\r\n            </article>\r\n        {{/if}}\r\n        <section class=\"footer-info\">\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-webPages\">\r\n                <h5 class=\"footer-info-heading\">Company Information</h5>\r\n                <ul class=\"footer-info-list company-info\">\r\n                  \r\n                  \r\n                    <li>\r\n                        <a href=\"/\">Home</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/about-us/\">About Us</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/delivery/\">Delivery</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/deal-match/\">Deal Match</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/faqs/\">FAQs</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/terms/\">Terms of Use</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/privacy/\">Privacy Policy</a>\r\n                    </li>\r\n                    <li>\r\n                        <a href=\"/product-demo/\">Demonstration</a>\r\n                    </li>\r\n                    \r\n                    \r\n                </ul>\r\n              <div class=\"logo-social\">\r\n                    <a href=\"https://www.facebook.com/greatbuysplus\" target=\"_blank\"><img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/footer/fb.png')}}\"></a>\r\n                    <a href=\"https://www.youtube.com/user/KingsGreatBuysPlus\" target=\"_blank\"><img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/footer/youtube.png')}}\"></a>\r\n                </div>\r\n            </article>\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-categories\">\r\n                <h5 class=\"footer-info-heading\">Shop Categories</h5>\r\n                <ul class=\"footer-info-list category-name\">\r\n                    {{#each categories}}\r\n                        <li>\r\n                            <a href=\"{{url}}\">{{name}}</a>\r\n                        </li>\r\n                    {{/each}}\r\n                </ul>\r\n                  <div class=\"accredited-img\">\r\n                   <a href=\"https://www.bbb.org/us/in/evansville/profile/major-appliance-dealers/kings-great-buys-plusindiana-0482-90856#bbbseal\" target=\"_blank\"><img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/footer/accredited.png')}}\"></a>\r\n                </div>\r\n            </article>\r\n\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"footer-brands\">\r\n                <h5 class=\"footer-info-heading\">Current Promotions</h5>\r\n                <ul class=\"footer-info-list\">\r\n                 \r\n                        <li>\r\n                            <a href=\"/current-ads/\">Current Ads</a>\r\n                        </li>\r\n                      <li>\r\n                            <a href=\"/rebates/\">Current Rebates</a>\r\n                        </li>\r\n                      <li>\r\n                            <a href=\"{{url}}\">Extreme Values</a>\r\n                        </li>\r\n                      <li>\r\n                            <a href=\"/financing/\">Financing</a>\r\n                        </li>\r\n                        <li>\r\n                            <a href=\"/disclaimer/\">Disclaimer</a>\r\n                        </li>\r\n\r\n                </ul>\r\n            </article>\r\n\r\n\r\n            <article class=\"footer-info-col footer-info-col--small\" data-section-type=\"storeInfo\">\r\n                <h1 class=\"footer-shop-heading\">Shop Smart.</h1>\r\n                <h1 class=\"footer-shop-heading last\">Shop Local.</h1>       \r\n                <a href=\"/rebates/\" class=\"footer-button\">SHOP NOW</a>\r\n            </article>\r\n     \r\n\r\n            \r\n        </section>\r\n\r\n       \r\n\r\n     \r\n   \r\n    </div>\r\n    \r\n      <div class=\"footer-copyright\">\r\n                <p class=\"powered-by\">&copy; {{moment format=\"YYYY\"}} {{settings.store_name}} </p>\r\n            </div>\r\n</footer>\r\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/subscription-form":"<h5>{{lang 'newsletter.subscribe'}}</h5>\n\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"Your Email\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n","components/products/sticky-product-compare":"<div class=\"maincontainer-compare\">\r\n         <div class=\"child-container-compare\" style=\"display:none\">\r\n                  <div class=\"action\" style=\"flex-direction:column;text-align:center;border:none !important;display:none;\">\r\n                           <h4 style=\"margin-top:0;font-size:18px !important;\" id=\"h4-compare\"></h4>\r\n                           <div style=\"display:block;width:100%;\">\r\n                                    <button style=\"color:white;background-color:#d22229; padding: 18px;font-size: 17px;border-radius: 11px;\" id=\"btn-compare-now\" onclick=\"submit_url()\">COMPARE</button>\r\n                           </div>\r\n                           <div style=\"display:block;text-align:center;margin-top:20px;\">\r\n                                    <button style=\"text-decoration: underline;cursor: pointer;\" onclick=\"reset()\">\r\n                                             Clear All\r\n                                    </button>\r\n                           </div>\r\n                  </div>   \r\n         </div>     \r\n</div>\r\n<script>\r\n         //compare products\r\n         var check_count = 0;\r\n         var arr_product = new Array();\r\n                  $(document).ready(function(){\r\n                           //unchecked all checkbox after page load XD\r\n                           $('.cmp').prop('checked',false);\r\n                  });\r\n                  $('.cmp').on('change',function(){\r\n                  $('.child-container-compare').css('display','flex');\r\n                  let product_image =  $(this).parents('li.product').children().eq(0).find('img.listItem-image').attr('src');\r\n                  let product_logo_title = $(this).parents('li.product').children().eq(1).find('.cardlogoimage').attr('src');\r\n                  let product_description = $(this).parents('li.product').children().eq(1).find('.listItem-title').children().html();\r\n                  let product_price = $(this).parents('li.product').children().eq(2).find('span.ptax').html();\r\n                  let id = $(this).attr('data-compare-id');\r\n                  var html ='';\r\n                  var check_count = $('input[type=\"checkbox\"]:checked').length;\r\n                  // var actions = '<div class=\"action\" style=\"display:flex;flex-direction:column;text-align:center;border:none !important;display:none;\"><h4 style=\"margin-top:0;font-size:18px !important;\">Comparing ('+len+') Products</h4></div>';       \r\n                  // var action = '<h4 style=\"margin-top:0;font-size:18px !important;\">Comparing ('+len+') Products</h4>';\r\n                  var elem = $('.child-container-compare');\r\n                  html = '<div id=\"compare-sticky-'+id+'\" class=\"compare-sticky\" style=\"height:190px;width:240px;border:1px solid #e2e2e2;position:relative;display:flex;justify-content:center;align-items:center;padding:5px;\" class=\"children-container-compare\"><button class=\"close-button\" onclick=\"delete_parent(this)\" style=\"top:1px;right:4px;position:absolute\" delete-id=\"'+id+'\"><span class=\"fa fa-close btn btn-dark btn-close\" data-id=\"'+id+\r\n                                             '\"></span></button><div style=\"position:relative;display:block;\"><img style=\"height:100px;\" src=\"'+ product_image +'\"></div>'+\r\n                                             '<div style=\"width:150px;display:block;padding-left:15px;\"><img style=\"width:100px;\" src=\"'+product_logo_title+'\"><span style=\"-webkit-box-orient: vertical;display:-webkit-box !important;-webkit-line-clamp: 3 !important;overflow: hidden !important;\">'+\r\n                                             product_description + '</span><span style=\"position:relative;font-size:19px;top:10px\">'+ product_price + '</span></div></div>';                       \r\n                  //get element if 2 checkbox was checked   \r\n                  if(this.checked){\r\n                           arr_product.push(\"/\"+id);\r\n                           elem.prepend(html);\r\n                           if(check_count > 1){\r\n                                   $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                                   $('.action').css('display','block');\r\n                           }  \r\n                  }else{\r\n                        $('#compare-sticky-'+id).remove();\r\n                        var index_remove = arr_product.indexOf(id);\r\n                        arr_product.splice(index_remove,1);\r\n                       \r\n                        if(check_count < 2){\r\n                                    $('.action').css('display','none');\r\n                        }else{\r\n                                    $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                        }\r\n                        if(check_count == 0){\r\n                              $('.child-container-compare').css('display','none');\r\n                        }\r\n                  } \r\n         });\r\n         function delete_parent(elem){\r\n                  \r\n                  let id =    elem.getAttribute('delete-id');\r\n                  $('#compare-sticky-'+id).remove();\r\n                  var index_remove = arr_product.indexOf(id);\r\n                  arr_product.splice(index_remove,1);\r\n                  //unchecked the checkedbox\r\n                \r\n                  $('#compare-'+id).prop('checked',false);\r\n                  var check_count = $('input[type=\"checkbox\"]:checked').length;\r\n                   if(check_count < 2){\r\n                            $('.action').css('display','none');\r\n                   }else{\r\n                            $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                   }\r\n                   if(check_count == 0){\r\n                         $('.child-container-compare').css('display','none');\r\n                   }\r\n                  //uncheck the checkbox\r\n         }\r\n         function submit_url(){\r\n                  var data = arr_product.toString();\r\n                  var regex = data.replace(/,/g,\"\");\r\n                  window.location.href = \"/compare\"+regex;\r\n         }\r\n         function reset(){\r\n               $(\".cmp\").prop('checked',false);\r\n               $('.action').css('display','none');\r\n               $('.compare-sticky').remove();\r\n               check_count = $('input[type=\"checkbox\"]:checked').length;\r\n               $('.child-container-compare').css('display','none');\r\n         }\r\n</script>"}
