onents/common/navigation-list-new}}\r\n                 </li>\r\n                  {{/all}}\r\n            {{/if}}\r\n        \r\n            {{#if ../theme_settings.navigation_design '===' 'alternate'}}\r\n             {{#all (if name '!==' 'Rebates') (if name '!==' 'Appliances') }}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/navigation-list-alternate}}\r\n                   </li>\r\n                  {{/all}}\r\n            {{/if}}\r\n        \r\n  \r\n        \r\n        {{/each}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"/brands/\" style=\"white-space:nowrap;\">\r\n                    <!-- <img class=\"nav-image\" src=\"{{getImage (cdn 'img/homepage/Brands.png')}}\"  /> -->\r\n                   <span style=\"white-space:nowrap\">Brands</span>\r\n                \r\n                </a>\r\n            </li>\r\n        <li class=\"navPages-item\">\r\n                <a href=\"/deal-match/\" class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" style=\"white-space:nowrap;\">\r\n                    <!-- <img class=\"nav-image\" src=\"{{getImage (cdn 'img/homepage/deal-match.png')}}\"  /> -->\r\n                    <span style=\"white-space: nowrap;\">Deal Match</span>\r\n                   \r\n                </a>\r\n        </li>\r\n        \r\n    \r\n        \r\n        \r\n    </ul>\r\n\r\n</nav>\r\n<script type=\"text/javascript\" src=\"https://code.jquery.com/git/jquery-1.x-git.min.js\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/jquery-migrate/1.2.1/jquery-migrate.min.js\" integrity=\"sha512-fDGBclS3HUysEBIKooKWFDEWWORoA20n60OwY7OSYgxGEew9s7NgDaPkj7gqQcVXnASPvZAiFW8DiytstdlGtQ==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>\r\n<script src=\"https://cdnjs.cloudflare.com/ajax/libs/slick-carousel/1.8.1/slick.min.js\" integrity=\"sha512-XtmMtDEcNz2j7ekrtHvOVR4iwwaD6o/FUJe6+Zq+HgcCsk3kj4uSQQR8weQ2QVj1o0Pk6PwYLohm206ZzNfubg==\" crossorigin=\"anonymous\" referrerpolicy=\"no-referrer\"></script>\r\n<script>\r\n\r\n    var slick_option = {\r\n        \"dots\": false,\r\n        \"mobileFirst\": true,\r\n        \"slidesToShow\": 5,\r\n        \"slidesToScroll\": 5,\r\n        \"autoplay\": false,\r\n\t\t\"infinite\": false\r\n    };\r\n\t\t\t\r\n\t\t\tjQuery('.gg').each(function(){\r\n\t\t\t\tjQuery(this).not('.slick-initialized').slick(slick_option);\r\n\t\t\t});\r\n\t\t\t\r\n\t\t\t\r\n\r\n\r\njQuery('.navPages-action.has-subMenu').on('hover click mousemove' , function(){\r\n\tvar _slickContainer = jQuery(this).parent().find('.gg');\r\n\t\r\n      _slickContainer[0].slick.refresh();\t \r\n    \r\n\r\n});\r\n\r\n\r\n\r\n$(window).on('resize orientationchange', function() {\r\n  $('.gg').slick('resize');\r\n});\r\n\r\n\r\n//delete element if  innerhtml of navpage is empty\r\n    var elem = document.getElementsByClassName('navPages-item');\r\n    for(let i = 0; i<=elem.length;i++){\r\n        var htmlstring = elem[i].innerHTML;\r\n        htmlstring = (htmlstring.trim) ? htmlstring.trim() : htmlstring.replace(/^\\s+/,'');\r\n        if(htmlstring == ''){\r\n            elem[i].remove();\r\n        }\r\n    }\r\n</script>","components/common/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n","components/common/navigation":"\r\n<nav class=\"navUser\">\r\n    {{> components/common/currency-selector}}\r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n            <li class=\"navUser-item\">\r\n                <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/location.png')}}\"> -->\r\n                <a href=\"/location/\"><i class=\"fa fa-map-marker navigation_icons\" aria-hidden=\"true\"></i></a>  \r\n                <a class=\"navUser-action\" href=\"/location/\">Find a Store</a>\r\n            </li>\r\n\r\n        <li class=\"navUser-item navUser-item--account\">\r\n            <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/account.png')}}\"> -->\r\n                <a href=\"{{urls.account.index}}\"><i class=\"fa fa-user-circle-o navigation_icons\" aria-hidden=\"true\"></i></a>\r\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a>\r\n        </li>\r\n        \r\n        <li class=\"navUser-item navUser-item--cart\">\r\n             <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/cart.png')}}\"> -->\r\n             <a href=\"{{urls.cart}}\"><i class=\"fa fa-shopping-cart navigation_icons\" aria-hidden=\"true\"></i></a>\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                data-dropdown=\"cart-preview-dropdown\"\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span> <span class=\"countPill cart-quantity\"></span>\r\n            </a>\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n    </ul>\r\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n</nav>\r\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list-alternate":"{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n","components/common/navigation-list-new":"{{#if children}}\r\n        <a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" style=\"white-space:nowrap;\">\r\n            <!-- <img class=\"nav-image\" src=\"{{cdn (concat 'img/homepage/' (concat name '.png'))}}\"  /> -->\r\n            <span style=\"white-space: nowrap;\">{{name}}</span>\r\n            <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\r\n        </a>\r\n\r\n    <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <div class=\"navChild_name\">\r\n            <h2>{{name}}</h2>\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\" style=\"margin-left:5% !important;\">SHOP ALL \r\n                    <img src=\"https://cdn11.bigcommerce.com/s-bek4r4rsz5/product_images/uploaded_images/chevron-right-solid-red-.png\">\r\n                </a>\r\n        </div> \r\n        <ul class=\"gg\">\r\n            {{#each children}}\r\n                <li>\r\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n                            <img class=\"\" src=\"{{cdn (concat 'img/nav/' (concat  (lowercase (dashcase name)) '.png'))}}\"  />\r\n                            <span style=\"margin-left:10%;\">{{name}}</span>\r\n                        </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul> \r\n    </div>\r\n{{else}}\r\n    {{#if name '==' 'Delivery Express'}}\r\n        <a href=\"/delivery-express/\" class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" style=\"white-space:nowrap;margin-right:2%;margin-left:1%;\">\r\n            <!-- <img class=\"nav-image\" src=\"{{cdn 'assets/img/homepage/delivery-express.png'}}\"  />  -->\r\n            <span style=\"white-space: nowrap;\">{{name}}</span>\r\n        </a>\r\n    {{/if}}\r\n{{/if}}\r\n\r\n\r\n","components/common/quick-search":"<div class=\"container\">\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n","components/common/search-header":"<form class=\"form\"  action=\"{{urls.search}}\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"search_query_adv\">{{lang 'search.results.form_label'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\" style=\"background-color:white;\">\r\n                <input class=\"form-input\" placeholder=\"What can we help you find?\" id=\"search_query_adv\" name=\"search_query_adv\" value=\"{{forms.search.query}}\">\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">\r\n            </div>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n","components/common/subscription-form":"<h5>{{lang 'newsletter.subscribe'}}</h5>\n\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"Your Email\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n","components/products/sticky-product-compare":"<div class=\"maincontainer-compare\">\r\n         <div class=\"child-container-compare\" style=\"display:none\">\r\n                  <div class=\"action\" style=\"flex-direction:column;text-align:center;border:none !important;display:none;\">\r\n                           <h4 style=\"margin-top:0;font-size:18px !important;\" id=\"h4-compare\"></h4>\r\n                           <div style=\"display:block;width:100%;\">\r\n                                    <button style=\"color:white;background-color:#d22229; padding: 18px;font-size: 17px;border-radius: 11px;\" id=\"btn-compare-now\" onclick=\"submit_url()\">COMPARE</button>\r\n                           </div>\r\n                           <div style=\"display:block;text-align:center;margin-top:20px;\">\r\n                                    <button style=\"text-decoration: underline;cursor: pointer;\" onclick=\"reset()\">\r\n                                             Clear All\r\n                                    </button>\r\n                           </div>\r\n                  </div>   \r\n         </div>     \r\n</div>\r\n<script>\r\n         //compare products\r\n         var check_count = 0;\r\n         var arr_product = new Array();\r\n                  $(document).ready(function(){\r\n                           //unchecked all checkbox after page load XD\r\n                           $('.cmp').prop('checked',false);\r\n                  });\r\n                  $('.cmp').on('change',function(){\r\n                  $('.child-container-compare').css('display','flex');\r\n                  let product_image =  $(this).parents('li.product').children().eq(0).find('img.listItem-image').attr('src');\r\n                  let product_logo_title = $(this).parents('li.product').children().eq(1).find('.cardlogoimage').attr('src');\r\n                  let product_description = $(this).parents('li.product').children().eq(1).find('.listItem-title').children().html();\r\n                  let product_price = $(this).parents('li.product').children().eq(2).find('span.ptax').html();\r\n                  let id = $(this).attr('data-compare-id');\r\n                  var html ='';\r\n                  var check_count = $('input[type=\"checkbox\"]:checked').length;\r\n                  // var actions = '<div class=\"action\" style=\"display:flex;flex-direction:column;text-align:center;border:none !important;display:none;\"><h4 style=\"margin-top:0;font-size:18px !important;\">Comparing ('+len+') Products</h4></div>';       \r\n                  // var action = '<h4 style=\"margin-top:0;font-size:18px !important;\">Comparing ('+len+') Products</h4>';\r\n                  var elem = $('.child-container-compare');\r\n                  html = '<div id=\"compare-sticky-'+id+'\" class=\"compare-sticky\" style=\"height:190px;width:240px;border:1px solid #e2e2e2;position:relative;display:flex;justify-content:center;align-items:center;padding:5px;\" class=\"children-container-compare\"><button class=\"close-button\" onclick=\"delete_parent(this)\" style=\"top:1px;right:4px;position:absolute\" delete-id=\"'+id+'\"><span class=\"fa fa-close btn btn-dark btn-close\" data-id=\"'+id+\r\n                                             '\"></span></button><div style=\"position:relative;display:block;\"><img style=\"height:100px;\" src=\"'+ product_image +'\"></div>'+\r\n                                             '<div style=\"width:150px;display:block;padding-left:15px;\"><img style=\"width:100px;\" src=\"'+product_logo_title+'\"><span style=\"-webkit-box-orient: vertical;display:-webkit-box !important;-webkit-line-clamp: 3 !important;overflow: hidden !important;\">'+\r\n                                             product_description + '</span><span style=\"position:relative;font-size:19px;top:10px\">'+ product_price + '</span></div></div>';                       \r\n                  //get element if 2 checkbox was checked   \r\n                  if(this.checked){\r\n                           arr_product.push(\"/\"+id);\r\n                           elem.prepend(html);\r\n                           if(check_count > 1){\r\n                                   $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                                   $('.action').css('display','block');\r\n                           }  \r\n                  }else{\r\n                        $('#compare-sticky-'+id).remove();\r\n                        var index_remove = arr_product.indexOf(id);\r\n                        arr_product.splice(index_remove,1);\r\n                       \r\n                        if(check_count < 2){\r\n                                    $('.action').css('display','none');\r\n                        }else{\r\n                                    $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                        }\r\n                        if(check_count == 0){\r\n                              $('.child-container-compare').css('display','none');\r\n                        }\r\n                  } \r\n         });\r\n         function delete_parent(elem){\r\n                  \r\n                  let id =    elem.getAttribute('delete-id');\r\n                  $('#compare-sticky-'+id).remove();\r\n                  var index_remove = arr_product.indexOf(id);\r\n                  arr_product.splice(index_remove,1);\r\n                  //unchecked the checkedbox\r\n                \r\n                  $('#compare-'+id).prop('checked',false);\r\n                  var check_count = $('input[type=\"checkbox\"]:checked').length;\r\n                   if(check_count < 2){\r\n                            $('.action').css('display','none');\r\n                   }else{\r\n                            $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                   }\r\n                   if(check_count == 0){\r\n                         $('.child-container-compare').css('display','none');\r\n                   }\r\n                  //uncheck the checkbox\r\n         }\r\n         function submit_url(){\r\n                  var data = arr_product.toString();\r\n                  var regex = data.replace(/,/g,\"\");\r\n                  window.location.href = \"/compare\"+regex;\r\n         }\r\n         function reset(){\r\n               $(\".cmp\").prop('checked',false);\r\n               $('.action').css('display','none');\r\n               $('.compare-sticky').remove();\r\n               check_count = $('input[type=\"checkbox\"]:checked').length;\r\n               $('.child-container-compare').css('display','none');\r\n         }\r\n</script>","layout/cms-base":"<!DOCTYPE html>\r\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\r\n    <head>\r\n        <title>{{ head.title }}</title>\r\n        {{{ resourceHints }}}\r\n        {{{ head.meta_tags }}}\r\n        {{{ head.config }}}\r\n        {{#block \"head\"}} {{/block}}\r\n\r\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1\">\r\n        <link href=\"https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css\" rel=\"stylesheet\">\r\n         <script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js\"></script>\r\n        <script src=\"jquery-3.4.1.min.js\"></script>\r\n        <link href=\"https://fonts.googleapis.com/icon?family=Material+Icons\"\r\n      rel=\"stylesheet\">\r\n       <link rel=\"stylesheet\" type=\"text/css\" href=\"https://cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css\">\r\n      <script type=\"text/javascript\" charset=\"utf8\" src=\"https://code.jquery.com/jquery-3.5.1.js\"></script>\r\n    <script type=\"text/javascript\" charset=\"utf8\" src=\"https://cdn.datatables.net/1.10.21/js/jquery.dataTables.min.js\"></script>\r\n    <script type=\"text/css\" charset=\"utf8\" src=\"https://cdn.datatables.net/1.10.21/css/jquery.dataTables.min.css\">\r\n        <script>\r\n            {{!-- Change document class from no-js to js so we can detect this in css --}}\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n        {{!-- Load Lazysizes script ASAP so images will appear --}}\r\n        <script>\r\n            {{!-- Only load visible elements until the onload event fires, after which preload nearby elements. --}}\r\n            window.lazySizesConfig = window.lazySizesConfig || {};\r\n            window.lazySizesConfig.loadMode = 1;\r\n        </script>\r\n        <script async src=\"{{cdn 'assets/dist/theme-bundle.head_async.js'}}\"></script>\r\n\r\n        {{ getFontsCollection }}\r\n        {{{stylesheet '/assets/css/theme.css'}}}\r\n\r\n        {{{head.scripts}}}\r\n\r\n        {{~inject 'themeSettings' theme_settings}}\r\n        {{~inject 'genericError' (lang 'common.generic_error')}}\r\n        {{~inject 'maintenanceMode' settings.maintenance}}\r\n        {{~inject 'urls' urls}}\r\n        {{~inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{~inject 'cartId' cart_id}}\r\n        {{~inject 'template' template}}\r\n    </head>\r\n    <body>\r\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\r\n\r\n        {{> components/common/header }}\r\n        {{> components/common/cms-body }}\r\n        {{> components/common/footer }}\r\n<button onclick=\"topFunction()\" id=\"myBtn\" title=\"Go to top\"><span class=\"material-icons\">\r\narrow_upward\r\n</span>\r\n\r\n</button>\r\n<script>// When the user scrolls down 20px from the top of the document, show the button\r\nwindow.onscroll = function() {\r\n  scrollFunction()\r\n};\r\n            \r\nvar navbar = document.getElementById(\"stick-navbar\");\r\nvar sticky = navbar.offsetTop;\r\n            \r\nfunction scrollFunction() {\r\n  if (document.body.scrollTop > 300 || document.documentElement.scrollTop > 300) {\r\n    document.getElementById(\"myBtn\").style.display = \"block\";\r\n    navbar.classList.add(\"sticky\")\r\n  } else {\r\n    document.getElementById(\"myBtn\").style.display = \"none\";\r\n    navbar.classList.remove(\"sticky\");\r\n  }\r\n}\r\n\r\n// When the user clicks on the button, scroll to the top of the document\r\nfunction topFunction() {\r\n  document.body.scrollTop = 0;\r\n  document.documentElement.scrollTop = 0;\r\n}</script>\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\r\n        <script>\r\n            {{!-- Exported in app.js --}}\r\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n        </script>\r\n\r\n        {{{footer.scripts}}}\r\n    </body>\r\n</html>\r\n","pages/custom/page/returns":"{{#partial \"page\"}}\r\n\r\n<main class=\"page cms\">\r\n    \r\n   <div class=\"returns-banner\">\r\n        <h1 class=\"page-heading\">{{ page.title }}</h1>\r\n    </div>\r\n\r\n    <div class=\"page-content returns\">\r\n        <h1>Returns and Exchange</h1>\r\n        <div class=\"returns-content\">\r\n           {{{page.content}}}\r\n        </div>\r\n    </div>\r\n\r\n\r\n</main>\r\n\r\n{{/partial}}\r\n\r\n{{> layout/cms-base}}\r\n"}
