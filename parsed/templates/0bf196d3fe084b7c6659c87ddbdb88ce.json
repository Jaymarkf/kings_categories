{"components/common/wishlist-dropdown":"<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist form-action\" data-wishlist-add method=\"post\">\n    <a aria-controls=\"wishlist-dropdown\" aria-expanded=\"false\" class=\"button dropdown-menu-button sub-button\" data-dropdown=\"wishlist-dropdown\" style=\"color: #f00;background-color: #fcd8df;border:none;\">\n        <span><i class=\"fa fa-heart-o\" aria-hidden=\"true\"></i>&nbsp;&nbsp;&nbsp;{{lang 'account.wishlists.add_item'}}</span>\n    </a>\n    <ul aria-hidden=\"true\" class=\"dropdown-menu\" data-dropdown-content id=\"wishlist-dropdown\" tabindex=\"-1\">\n        {{#if customer.wishlists}} {{#each customer.wishlists}}\n        <li>\n            <input class=\"button {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\" formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\n                type=\"submit\" value=\"{{this.name}}\">\n            \n        </li>\n        {{/each}} {{else}}\n        <li>\n            <input class=\"button\" type=\"submit\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\n        </li>\n        {{/if}}\n        <li>\n            <a data-wishlist class=\"button sub-button\" href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\">{{lang 'account.wishlists.create_new'}}</a>\n        </li>\n    </ul>\n</form>\n"}
