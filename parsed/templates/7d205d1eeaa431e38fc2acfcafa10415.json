].innerHTML;\r\n        htmlstring = (htmlstring.trim) ? htmlstring.trim() : htmlstring.replace(/^\\s+/,'');\r\n        if(htmlstring == ''){\r\n            elem[i].remove();\r\n        }\r\n    }\r\n</script>","components/common/modal":"<div id=\"modal\" class=\"modal\" data-reveal data-prevent-quick-search-close>\n    <a href=\"#\" class=\"modal-close\" aria-label=\"{{lang 'common.close'}}\" role=\"button\">\n        <span aria-hidden=\"true\">&#215;</span>\n    </a>\n    <div class=\"modal-content\"></div>\n    <div class=\"loadingOverlay\"></div>\n</div>\n","components/common/navigation":"\r\n<nav class=\"navUser\">\r\n    {{> components/common/currency-selector}}\r\n    <ul class=\"navUser-section navUser-section--alt\">\r\n            <li class=\"navUser-item\">\r\n                <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/location.png')}}\"> -->\r\n                <a href=\"/location/\"><i class=\"fa fa-map-marker navigation_icons\" aria-hidden=\"true\"></i></a>  \r\n                <a class=\"navUser-action\" href=\"/location/\">Find a Store</a>\r\n            </li>\r\n\r\n        <li class=\"navUser-item navUser-item--account\">\r\n            <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/account.png')}}\"> -->\r\n                <a href=\"{{urls.account.index}}\"><i class=\"fa fa-user-circle-o navigation_icons\" aria-hidden=\"true\"></i></a>\r\n                <a class=\"navUser-action\" href=\"{{urls.account.index}}\">{{lang 'common.account'}}</a>\r\n        </li>\r\n        \r\n        <li class=\"navUser-item navUser-item--cart\">\r\n             <!-- <img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/header/cart.png')}}\"> -->\r\n             <a href=\"{{urls.cart}}\"><i class=\"fa fa-shopping-cart navigation_icons\" aria-hidden=\"true\"></i></a>\r\n            <a\r\n                class=\"navUser-action\"\r\n                data-cart-preview\r\n                data-dropdown=\"cart-preview-dropdown\"\r\n                data-options=\"align:right\"\r\n                href=\"{{urls.cart}}\">\r\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span> <span class=\"countPill cart-quantity\"></span>\r\n            </a>\r\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\r\n        </li>\r\n    </ul>\r\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\r\n        {{> components/common/quick-search}}\r\n    </div>\r\n</nav>\r\n","components/common/navigation-dropdown":"<div class=\"navPage-subMenu navPage-subMenu-horizontal\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-parent\">\n            <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max\" href=\"{{url}}\">{{lang 'category.view_all.name' category=name}}</a>\n        </li>\n        {{#each children}}\n            <li class=\"navPage-subMenu-item-child\">\n                {{#if children}}\n                    <a class=\"navPage-subMenu-action navPages-action navPages-action-depth-max has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n                        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"></i>\n                    </a>\n                    {{> components/common/navigation-dropdown}}\n                {{else}}\n                    <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n                {{/if}}\n            </li>\n        {{/each}}\n    </ul>\n</div>\n","components/common/navigation-list-alternate":"{{#if children}}\n    <a class=\"navPages-action navPages-action-depth-max has-subMenu is-root{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" data-collapsible=\"navPages-{{id}}\">\n        {{name}} <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\"><svg><use xlink:href=\"#icon-chevron-down\" /></svg></i>\n    </a>\n    {{> components/common/navigation-dropdown}}\n{{else}}\n    <a class=\"navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">{{name}}</a>\n{{/if}}\n","components/common/navigation-list-new":"{{#if children}}\r\n        <a class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\" style=\"white-space:nowrap;\">\r\n            <!-- <img class=\"nav-image\" src=\"{{cdn (concat 'img/homepage/' (concat name '.png'))}}\"  /> -->\r\n            <span style=\"white-space: nowrap;\">{{name}}</span>\r\n            <i class=\"fa fa-chevron-down\" aria-hidden=\"true\"></i>\r\n        </a>\r\n\r\n    <div class=\"navPage-subMenu\" id=\"navPages-{{id}}\" aria-hidden=\"true\" tabindex=\"-1\">\r\n        <div class=\"navChild_name\">\r\n            <h2>{{name}}</h2>\r\n                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{url}}\" style=\"margin-left:5% !important;\">SHOP ALL \r\n                    <img src=\"https://cdn11.bigcommerce.com/s-bek4r4rsz5/product_images/uploaded_images/chevron-right-solid-red-.png\">\r\n                </a>\r\n        </div> \r\n        <ul class=\"gg\">\r\n            {{#each children}}\r\n                <li>\r\n                        <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} activePage{{/if}}\" href=\"{{url}}\">\r\n                            <img class=\"\" src=\"{{cdn (concat 'img/nav/' (concat  (lowercase (dashcase name)) '.png'))}}\"  />\r\n                            <span style=\"margin-left:10%;\">{{name}}</span>\r\n                        </a>\r\n                </li>\r\n            {{/each}}\r\n        </ul> \r\n    </div>\r\n{{else}}\r\n    {{#if name '==' 'Delivery Express'}}\r\n        <a href=\"/delivery-express/\" class=\"navPages-action has-subMenu{{#if is_active}} activePage{{/if}}\" style=\"white-space:nowrap;margin-right:2%;margin-left:1%;\">\r\n            <!-- <img class=\"nav-image\" src=\"{{cdn 'assets/img/homepage/delivery-express.png'}}\"  />  -->\r\n            <span style=\"white-space: nowrap;\">{{name}}</span>\r\n        </a>\r\n    {{/if}}\r\n{{/if}}\r\n\r\n\r\n","components/common/quick-search":"<div class=\"container\">\n    <form class=\"form\" action=\"{{urls.search}}\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n</div>\n","components/common/responsive-img":"{{!--\nThis file implements the lazysizes srcset pattern (as seen on\nhttps://github.com/aFarkas/lazysizes#modern-transparent-srcset-pattern) for maximum performance on modern browsers\nand reasonable compatibility with legacy browsers.\n\nThere are a few important arguments to know about when using this component:\n`lazyload`:\n    - If set to 'lazyload+lqip', will load a low-res image which will be replaced by lazysizes - ensuring that SOME\n    content is shown immediately on page load. Without LQIP, a transparent gif will be used instead, which reduces\n    the number of requests needed to load the page, making it even faster.\n    - If set to 'lazyload', the browser will not load the full-size image at first, instead the lazysizes plugin will\n        load it progressively after page load. This improves the load speed of the page, but it should be disabled for\n        high-priority above-the-fold images.\n    - If set to 'disabled', will not attempt to lazyload the image and instead tell the browser to load it immediately.\n        Use this for high-priority images that are above the fold.\n\n`fallback_size`:\n    This is the size to use for this image in legacy browsers that do not support srcset.\n    Can be defined as a pixel bounding-box size (e.g. \"123x123) or inherent width (e.g. \"123w\").\n\n`class`:\n    The CSS classes to add to the image, e.g. \"card-image\". If lazyloading is enabled, all images will have the\n    \"lazyload\" class so that lazysizes will target them automatically.\n\n`otherAttributes`:\n    Any other HTML attributes you want on the img tag, for example \"height='100' width='100'\"\n\n`default_image`:\n    The default image to use if `image` is undefined or falsy. Should be a theme asset, usually defined as a relative\n    path in the theme config. If a default image is not provided, you'll get an image tag with no image if the primary\n    image is undefined.\n\n`lqip_size`:\n    If you want to specify a particular size for the LQIP image, you can do so with this argument. A default of 80\n    pixels wide will be used otherwise. This argument has no effect if the lazyload mode is not \"lazyload+lqip\".\n\n--}}\n<img {{#if image}}{{!-- Get the default image for legacy browsers that do not support srcset\n--}}src=\"{{getImageSrcset image 1x=(default fallback_size '160w')}}\" alt=\"{{image.alt}}\" title=\"{{image.alt}}\" {{!--\n  Allow lazysizes to generate the 'sizes' attribute automatically --}}data-sizes=\"auto\"\n  {{#unless lazyload '==' 'disabled'}}\n    {{#if lazyload '==' 'lazyload+lqip'}}\n    {{!-- If we want a LQIP, include it as the default srcset (will be overwritten by lazysizes) --}}\n    srcset=\"{{getImageSrcset image 1x=(default lqip_size '1452w')}}\"\n    {{else}}\n      {{!-- If we don't want a LQIP, use a 1px transparent gif for the stand-in image\n            This requires no additional network request, so it's fast --}}\n    srcset=\"data:image/gif;base64,R0lGODlhAQABAIAAAP///wAAACH5BAEAAAAALAAAAAABAAEAAAICRAEAOw==\"\n    {{/if}}\n{{/unless}}\n{{!-- If lazyloading is disabled, just use \"srcset\". If it is not disabled, use \"data-srcset\" so lazysizes will use\n      this after page load --}}\n{{!--\nGenerate the srcset string using the default sizes defined in the helper. This will allow the browser to\npick the best-sized image based on its own understanding of how it will render the page.\n--}}\n\n\n{{#unless lazyload '==' 'disabled'}}data-{{/unless}}srcset=\"{{getImageSrcset image use_default_sizes=true }}\" \n{{else if default_image}}src=\"{{cdn default_image}}\"{{/if}}\nclass=\"{{#unless lazyload '==' 'disabled'}}lazyload{{/unless}}{{#if class}} {{class}}{{/if}}\"\n{{#if lazyload '==' 'lazyload'}}loading=\"lazy\"{{/if}}\n{{otherAttributes}} />\n","components/common/search-header":"<form class=\"form\"  action=\"{{urls.search}}\">\r\n    <fieldset class=\"form-fieldset\">\r\n        <div class=\"form-field\">\r\n            <label class=\"form-label is-srOnly\" for=\"search_query_adv\">{{lang 'search.results.form_label'}}</label>\r\n            <div class=\"form-prefixPostfix wrap\" style=\"background-color:white;\">\r\n                <input class=\"form-input\" placeholder=\"What can we help you find?\" id=\"search_query_adv\" name=\"search_query_adv\" value=\"{{forms.search.query}}\">\r\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"\">\r\n            </div>\r\n        </div>\r\n    </fieldset>\r\n</form>\r\n","components/common/social-links":"{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n","components/common/store-logo":"<a href=\"{{urls.home}}\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n","components/common/subscription-form":"<h5>{{lang 'newsletter.subscribe'}}</h5>\n\n\n<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n    <fieldset class=\"form-fieldset\">\n        <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n        <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n        <input type=\"hidden\" name=\"check\" value=\"1\">\n        <div class=\"form-field\">\n            <label class=\"form-label is-srOnly\" for=\"nl_email\">{{lang 'common.email_address'}}</label>\n            <div class=\"form-prefixPostfix wrap\">\n                <input class=\"form-input\" id=\"nl_email\" name=\"nl_email\" type=\"email\" value=\"{{customer.email}}\" placeholder=\"Your Email\">\n                <input class=\"button button--primary form-prefixPostfix-button--postfix\" type=\"submit\" value=\"{{lang 'newsletter.subscribe_submit'}}\">\n            </div>\n            {{#if settings.show_newsletter_summary }}\n                <div class=\"footer-newsletter-summary\">{{settings.newsletter_summary}}</div>\n            {{/if}}\n        </div>\n    </fieldset>\n</form>\n","components/products/sticky-product-compare":"<div class=\"maincontainer-compare\">\r\n         <div class=\"child-container-compare\" style=\"display:none\">\r\n                  <div class=\"action\" style=\"flex-direction:column;text-align:center;border:none !important;display:none;\">\r\n                           <h4 style=\"margin-top:0;font-size:18px !important;\" id=\"h4-compare\"></h4>\r\n                           <div style=\"display:block;width:100%;\">\r\n                                    <button style=\"color:white;background-color:#d22229; padding: 18px;font-size: 17px;border-radius: 11px;\" id=\"btn-compare-now\" onclick=\"submit_url()\">COMPARE</button>\r\n                           </div>\r\n                           <div style=\"display:block;text-align:center;margin-top:20px;\">\r\n                                    <button style=\"text-decoration: underline;cursor: pointer;\" onclick=\"reset()\">\r\n                                             Clear All\r\n                                    </button>\r\n                           </div>\r\n                  </div>   \r\n         </div>     \r\n</div>\r\n<script>\r\n         //compare products\r\n         var check_count = 0;\r\n         var arr_product = new Array();\r\n                  $(document).ready(function(){\r\n                           //unchecked all checkbox after page load XD\r\n                           $('.cmp').prop('checked',false);\r\n                  });\r\n                  $('.cmp').on('change',function(){\r\n                  $('.child-container-compare').css('display','flex');\r\n                  let product_image =  $(this).parents('li.product').children().eq(0).find('img.listItem-image').attr('src');\r\n                  let product_logo_title = $(this).parents('li.product').children().eq(1).find('.cardlogoimage').attr('src');\r\n                  let product_description = $(this).parents('li.product').children().eq(1).find('.listItem-title').children().html();\r\n                  let product_price = $(this).parents('li.product').children().eq(2).find('span.ptax').html();\r\n                  let id = $(this).attr('data-compare-id');\r\n                  var html ='';\r\n                  var check_count = $('input[type=\"checkbox\"]:checked').length;\r\n                  // var actions = '<div class=\"action\" style=\"display:flex;flex-direction:column;text-align:center;border:none !important;display:none;\"><h4 style=\"margin-top:0;font-size:18px !important;\">Comparing ('+len+') Products</h4></div>';       \r\n                  // var action = '<h4 style=\"margin-top:0;font-size:18px !important;\">Comparing ('+len+') Products</h4>';\r\n                  var elem = $('.child-container-compare');\r\n                  html = '<div id=\"compare-sticky-'+id+'\" class=\"compare-sticky\" style=\"height:190px;width:240px;border:1px solid #e2e2e2;position:relative;display:flex;justify-content:center;align-items:center;padding:5px;\" class=\"children-container-compare\"><button class=\"close-button\" onclick=\"delete_parent(this)\" style=\"top:1px;right:4px;position:absolute\" delete-id=\"'+id+'\"><span class=\"fa fa-close btn btn-dark btn-close\" data-id=\"'+id+\r\n                                             '\"></span></button><div style=\"position:relative;display:block;\"><img style=\"height:100px;\" src=\"'+ product_image +'\"></div>'+\r\n                                             '<div style=\"width:150px;display:block;padding-left:15px;\"><img style=\"width:100px;\" src=\"'+product_logo_title+'\"><span style=\"-webkit-box-orient: vertical;display:-webkit-box !important;-webkit-line-clamp: 3 !important;overflow: hidden !important;\">'+\r\n                                             product_description + '</span><span style=\"position:relative;font-size:19px;top:10px\">'+ product_price + '</span></div></div>';                       \r\n                  //get element if 2 checkbox was checked   \r\n                  if(this.checked){\r\n                           arr_product.push(\"/\"+id);\r\n                           elem.prepend(html);\r\n                           if(check_count > 1){\r\n                                   $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                                   $('.action').css('display','block');\r\n                           }  \r\n                  }else{\r\n                        $('#compare-sticky-'+id).remove();\r\n                        var index_remove = arr_product.indexOf(id);\r\n                        arr_product.splice(index_remove,1);\r\n                       \r\n                        if(check_count < 2){\r\n                                    $('.action').css('display','none');\r\n                        }else{\r\n                                    $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                        }\r\n                        if(check_count == 0){\r\n                              $('.child-container-compare').css('display','none');\r\n                        }\r\n                  } \r\n         });\r\n         function delete_parent(elem){\r\n                  \r\n                  let id =    elem.getAttribute('delete-id');\r\n                  $('#compare-sticky-'+id).remove();\r\n                  var index_remove = arr_product.indexOf(id);\r\n                  arr_product.splice(index_remove,1);\r\n                  //unchecked the checkedbox\r\n                \r\n                  $('#compare-'+id).prop('checked',false);\r\n                  var check_count = $('input[type=\"checkbox\"]:checked').length;\r\n                   if(check_count < 2){\r\n                            $('.action').css('display','none');\r\n                   }else{\r\n                            $('#h4-compare').html('Comparing ('+check_count+') Products');\r\n                   }\r\n                   if(check_count == 0){\r\n                         $('.child-container-compare').css('display','none');\r\n                   }\r\n                  //uncheck the checkbox\r\n         }\r\n         function submit_url(){\r\n                  var data = arr_product.toString();\r\n                  var regex = data.replace(/,/g,\"\");\r\n                  window.location.href = \"/compare\"+regex;\r\n         }\r\n         function reset(){\r\n               $(\".cmp\").prop('checked',false);\r\n               $('.action').css('display','none');\r\n               $('.compare-sticky').remove();\r\n               check_count = $('input[type=\"checkbox\"]:checked').length;\r\n               $('.child-container-compare').css('display','none');\r\n         }\r\n</script>","layout/category-base":"<!DOCTYPE html>\r\n<html class=\"no-js\" lang=\"{{ locale_name }}\">\r\n    <head>\r\n        <title>{{ category.name }}</title>\r\n        {{{ resourceHints }}}\r\n        {{{ head.meta_tags }}}\r\n        {{{ head.config }}}\r\n        {{#block \"head\"}} {{/block}}\r\n\r\n        <link href=\"{{ head.favicon }}\" rel=\"shortcut icon\">\r\n        <meta name=\"viewport\" content=\"width=device-width, initial-scale=1, maximum-scale=1\">\r\n        <link rel=\"stylesheet\" href=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css\">\r\n        <script src=\"https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js\"></script>\r\n        <script src=\"https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js\"></script>\r\n        <link href=\"https://fonts.googleapis.com/icon?family=Material+Icons\"\r\n      rel=\"stylesheet\">\r\n      <link rel=\"stylesheet\" href=\"https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css\">\r\n      <!-- <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script> -->\r\n        <script>\r\n            {{!-- Change document class from no-js to js so we can detect this in css --}}\r\n            document.documentElement.className = document.documentElement.className.replace('no-js', 'js');\r\n        </script>\r\n        {{!-- Load Lazysizes script ASAP so images will appear --}}\r\n        <script>\r\n            {{!-- Only load visible elements until the onload event fires, after which preload nearby elements. --}}\r\n            window.lazySizesConfig = window.lazySizesConfig || {};\r\n            window.lazySizesConfig.loadMode = 1;\r\n        </script>\r\n        <script async src=\"{{cdn 'assets/dist/theme-bundle.head_async.js'}}\"></script>\r\n\r\n        {{ getFontsCollection }}\r\n        {{{stylesheet '/assets/css/theme.css'}}}\r\n\r\n        {{{head.scripts}}}\r\n\r\n        {{~inject 'themeSettings' theme_settings}}\r\n        {{~inject 'genericError' (lang 'common.generic_error')}}\r\n        {{~inject 'maintenanceMode' settings.maintenance}}\r\n        {{~inject 'urls' urls}}\r\n        {{~inject 'secureBaseUrl' settings.secure_base_url}}\r\n        {{~inject 'cartId' cart_id}}\r\n        {{~inject 'template' template}}\r\n    </head>\r\n    <body>\r\n        <svg data-src=\"{{cdn 'img/icon-sprite.svg'}}\" class=\"icons-svg-sprite\"></svg>\r\n\r\n        {{> components/common/header }}\r\n        {{> components/common/categ-body }}\r\n        {{> components/common/footer }}\r\n<button onclick=\"topFunction()\" id=\"myBtn\" title=\"Go to top\"><span class=\"material-icons\">\r\narrow_upward\r\n</span>\r\n\r\n</button>\r\n<script>// When the user scrolls down 20px from the top of the document, show the button\r\nwindow.onscroll = function() {\r\n  scrollFunction()\r\n};\r\n            \r\nvar navbar = document.getElementById(\"stick-navbar\");\r\nvar sticky = navbar.offsetTop;\r\n            \r\nfunction scrollFunction() {\r\n  if (document.body.scrollTop > 300 || document.documentElement.scrollTop > 300) {\r\n    document.getElementById(\"myBtn\").style.display = \"block\";\r\n    navbar.classList.add(\"sticky\")\r\n  } else {\r\n    document.getElementById(\"myBtn\").style.display = \"none\";\r\n    navbar.classList.remove(\"sticky\");\r\n  }\r\n}\r\n\r\n// When the user clicks on the button, scroll to the top of the document\r\nfunction topFunction() {\r\n  document.body.scrollTop = 0;\r\n  document.documentElement.scrollTop = 0;\r\n}</script>\r\n        <script>window.__webpack_public_path__ = \"{{cdn 'assets/dist/'}}\";</script>\r\n        <script src=\"{{cdn 'assets/dist/theme-bundle.main.js'}}\"></script>\r\n        <script>\r\n            {{!-- Exported in app.js --}}\r\n            window.stencilBootstrap(\"{{page_type}}\", {{jsContext}}).load();\r\n        </script>\r\n\r\n        {{{footer.scripts}}}\r\n    </body>\r\n</html>\r\n","pages/custom/category/category-landing":"\r\n---\r\ncategory:\r\n    shop_by_price: true\r\n    products:\r\n        limit: {{theme_settings.categorypage_products_per_page}}\r\n---\r\n{{inject \"categoryProductsPerPage\" theme_settings.categorypage_products_per_page}}\r\n{{#partial \"head\"}}\r\n    {{#if pagination.category.previous}}\r\n        <link rel=\"prev\" href=\"{{pagination.category.previous}}\">\r\n    {{/if}}\r\n    {{#if pagination.category.next}}\r\n        <link rel=\"next\" href=\"{{pagination.category.next}}\">\r\n    {{/if}}\r\n{{/partial}}\r\n\r\n{{#partial \"page\"}}\r\n\r\n\r\n\r\n\r\n<!-- test css override -->\r\n<style>\r\n\t.container.categ-container{ max-width: 100% !important;}\r\n\t.body.categ-body { margin:0; }\r\n\t\r\n\r\n\t\r\n</style>\r\n\r\n\r\n<!-- sample promo banner -->\r\n<div style=\"width:100vw;position:relative;margin:0;padding:0;\">\r\n    <div class=\"Promo-Banner\" style=\"background: url('/content/category/images/{{dashcase category.name}}/banner-{{dashcase category.name}}.png') center;\r\n    height: 28vw;\r\n    background-repeat: no-repeat;\r\n    background-size: 100vw 28vw;\r\n    background-position: top;\r\n    \" id=\"Promo-Banner\">\r\n\r\n    </div>\r\n</div>\r\n<div class=\"position-abs\">\r\n    <div class=\"container-text-banner\">\r\n        <h1 style=\"text-align: left; padding-bottom: 10px; font-weight: bold; font-size: 3rem;\" id=\"title-ban\">{{category.name}}</h1>\r\n        <div class=\"row\" style=\"text-align:left;\">\r\n            <span style=\"color:white;font-size:1.7rem;text-align:left;\" id=\"desc\">\r\n                {{#if category.name '==' 'Refrigerators' }}\r\n                    Looking for a Samsung refrigerator, a<br>compact Frigidaire, or a Whirlpool  chest freezer?<br>King’s is the best appliance store<br>to buy your next fridge, cooler, or specialty storage!\r\n                {{else if category.name '==' 'Cooking'}}\r\n                    King’s Great Buys Plus is the best cooking<br> appliance store to buy your kitchen essentials! <br>Get a new gas stove from Maytag<br>or an over-the-range microwave from Whirlpool.\r\n                {{else if category.name '==' 'Dishwashers'}}\r\n                    Get your dream portable or built-in dishwasher<br>from the best appliance store near you – King’s! <br>We offer units from top brands Samsung, LG, Bosch, and Whirlpool.\r\n                {{else if category.name '==' 'Laundry'}}\r\n                    Make your laundry days convenient<br>with the best top load washers and<br> affordable front load dryers from <br>Samsung, LG, Maytag, Whirlpool, and GE Appliances.\r\n                {{else if category.name '==' 'Bedding'}}\r\n                    Shop for the best bedding products from King’s!<br>Browse our affordable mattresses by comfort level,<br>size, or construction. Sealy, Serta, and Tempur-Pedic also available.\r\n                {{else if category.name '==' 'Electronics'}}\r\n                    King’s is the new home of Hisense Smart TVs! Plus,<br> a wide selection of the latest smart TVs and audio systems from Samsung, LG, and Klipsch are also here at King’s. \r\n                {{else if category.name '==' 'Furniture'}}\r\n                    Furnish your home with discount items and awesome deals <br> from the best furniture store in town. <br>King’s offers items from trusted furniture makers: Southern Motion, <br>Liberty Furniture, and many more.\r\n                {{/if}}\r\n            </span>\r\n        </div>\r\n    </div>\r\n</div>\r\n\r\n{{> components/common/breadcrumbs breadcrumbs=breadcrumbs}}\r\n\r\n\r\n{{#if category.image}}\r\n    {{> components/common/responsive-img\r\n        image=category.image\r\n        fallback_size=theme_settings.zoom_size\r\n        lazyload=theme_settings.lazyload_mode\r\n        class=\"category-header-image\"\r\n    }}\r\n{{/if}}\r\n{{#unless theme_settings.hide_category_page_heading }}\r\n      \r\n    \r\n{{/unless}}\r\n{{{category.description}}}\r\n\r\n<script src=\"https://scripts.sirv.com/sirvjs/v3/sirv.js\"></script>\r\n<div class=\"page container\">\r\n    <style>\r\n  \r\n        .category-box{\r\n            padding:0 !important;\r\n            border-radius: 0  !important;\r\n            margin: 1% 1% !important;\r\n            position:relative;\r\n            border: 2px solid #d2d2d2;\r\n            width:31.333333%;\r\n\r\n        }\r\n        .category-box a{\r\n            text-decoration: none;\r\n        }\r\n      .category-box img.image-hover {\r\n      position: absolute;\r\n      top: 0;\r\n      right: 0;\r\n      left: 0;\r\n      bottom: 0;\r\n      object-fit: contain;\r\n      opacity: 0;\r\n      transition: opacity .1s;\r\n    }\r\n  \r\n    .hoverable:hover img.image-hover {\r\n      opacity: 1;\r\n    }\r\n    /* .category-box:hover .x{\r\n        display:none !important;\r\n    } */\r\n    .categ-desc{\r\n        position:relative;\r\n        display:block;\r\n        text-align:center;\r\n        display:flex !important;align-items: center;\r\n        justify-content: center;\r\n        margin-top:5%;\r\n        padding-bottom:5%;\r\n        padding-right:5%;\r\n        padding-left:5%;\r\n    }\r\n    html,body{\r\n        height:100%;\r\n    }\r\n    .img-container{\r\n        background-color:#f0f0f0;\r\n    }\r\n    .row-block{\r\n        display:block !important;\r\n      \r\n    }\r\n    .tx a{\r\n        text-decoration: underline;\r\n    }\r\n    .pz:hover .z{\r\n        opacity: 0;\r\n    }\r\n    </style>\r\n\r\n <main class=\"page-content\" id=\"product-listing-container\">\r\n     <a href=\"/at-t/\"><img src=\"{{getImage image 'thumb_size' (cdn 'assets/img/category/categ-banner.jpg')}}\"></a>\r\n     \r\n     \r\n    {{#each categories}}\r\n    \r\n    {{#if ../category.name '==' name }}\r\n          <h2 class=\"landing-title\"></h2>\r\n        {{#if children}}\r\n       <div class=\"main-landing\">\r\n            {{#each children}}\r\n\t\t\t <div class=\"category-box\">\r\n                <a href=\"{{url}}\" class=\"hoverable pz\">\r\n                    <div class=\"img-container\">\r\n                        <img class=\"Sirv image-main z\" style=\"width:100%\" data-sizes=\"auto\" src=\"/content/category/images/{{dashcase ../../../../category.name}}/{{dashcase name}}-image-only.png\">\r\n                        <img class=\"Sirv image-hover\" style=\"width:100%\" data-sizes=\"auto\" src=\"/content/category/images/{{dashcase ../../../../category.name}}/{{dashcase name}}-flip.png\"> \r\n                    </div>\r\n                </a>\r\n                <div class=\"categ-desc\">\r\n                    <div>\r\n                        <h1 class=\"row x ta\">{{ name }}</h1>\r\n                        <span class=\"row x tx\">\r\n                            {{#if children}}\r\n\t\t\t\t\t\t\t\t{{#each children}}\r\n\t\t\t\t\t\t\t\t\t<div class=\"row-block\"><a href=\"{{url}}\">{{name}}</a></div>\r\n\r\n\t\t\t\t\t\t\t\t{{/each}}\r\n\t\t\t\t\t\t\t{{/if}}\r\n                        </span>\r\n                    </div>    \r\n                </div>\r\n            </div>\t\t\r\n             {{/each}}\r\n        </div>\r\n        {{/if}}\r\n    {{/if}}\r\n    {{/each}}\r\n    </main>\r\n</div>\r\n\r\n{{/partial}}\r\n{{> layout/category-base}}\r\n"}
