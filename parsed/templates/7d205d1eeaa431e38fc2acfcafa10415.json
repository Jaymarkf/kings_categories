nt.getElementById('covid101');\r\n\r\n// When the user clicks anywhere outside of the modal, close it\r\nwindow.onclick = function(event) {\r\n  if (event.target == modal) {\r\n    modal.style.display = \"none\";\r\n  }\r\n}\r\n</script>\r\n<nav class=\"navUser\">\r\n    {{> components/common/currency-selector}}\r\n<div class=\"header-text\">\r\n    <span>Missing some items? <button class=\"covid101button\" onclick=\"document.getElementById('covid101').style.display='block'\">Learn more about the effects of COVID-19 on our supplies here.</button></span>\r\n</div>\r\n    \r\n   \r\n\r\n\r\n    \r\n    <ul class=\"custom-nav-banner navUser-section navUser-section--alt\">\r\n        \r\n            <li class=\"navUser-item\">\r\n            <a class=\"navUser-action navUser-item--compare\" href=\"{{urls.compare}}\" data-compare-nav>{{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span></a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/location/\">Locations</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/disclaimer/\">Disclaimer</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/rebates/\">Rebates</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/current-ads/\">Current Ads</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/blog/\">&nbsp; Blog &nbsp;</a>\r\n            </li>\r\n            <li class=\"navUser-item\">\r\n                <a class=\"navUser-action\" href=\"/contact-us/\">Contact Us</a>\r\n            </li>\r\n\r\n    </ul>\r\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-clos